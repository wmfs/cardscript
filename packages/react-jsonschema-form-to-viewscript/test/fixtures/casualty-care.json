{
  "jsonSchema": {
    "schema": {
      "formtitle": "Casualty Care",
      "formdescription": "This is an early-access version of a form to collect 'Casualty Care' data. Please note both form content and this app may change considerably ahead of a wider roll-out.",
      "formimage": "wmfs/casualty-care-background.jpg",
      "formcolorscheme": "lightblue",
      "type": "object",
      "properties": {
        "incident": {
          "type": "object",
          "title": "Incident details",
          "description": "Please provide details of the incident at which casualty care was administered (we'll be automatically linking to real-time incident data soon).",
          "required": ["incidentNum", "station"],
          "properties": {
            "incidentNum": {
              "type": "number",
              "title": "Incident Number"
            },
            "station": {
              "type": "string",
              "title": "Station",
              "enum": [
                "Aldridge",
                "Aston",
                "Bickenhill",
                "Billesley",
                "Bilston",
                "Binley",
                "Bloxwich",
                "Bournbrook",
                "Brierley Hill",
                "Canley",
                "Coventry",
                "Dudley",
                "Erdington",
                "Fallings Park",
                "Foleshill",
                "Haden Cross",
                "Handsworth",
                "Hay Mills",
                "Highgate",
                "Kings Norton",
                "Ladywood",
                "Northfield",
                "Oldbury",
                "Perry Barr",
                "Sheldon",
                "Smethwick",
                "Solihull",
                "Stourbridge",
                "Sutton Coldfield",
                "Tettenhall",
                "Tipton",
                "Walsall",
                "Ward End",
                "Wednesbury",
                "West Bromwich",
                "Willenhall",
                "Wolverhampton",
                "Woodgate Valley"
              ],
              "enumNames": [
                "Aldridge",
                "Aston",
                "Bickenhill",
                "Billesley",
                "Bilston",
                "Binley",
                "Bloxwich",
                "Bournbrook",
                "Brierley Hill",
                "Canley",
                "Coventry",
                "Dudley",
                "Erdington",
                "Fallings Park",
                "Foleshill",
                "Haden Cross",
                "Handsworth",
                "Hay Mills",
                "Highgate",
                "Kings Norton",
                "Ladywood",
                "Northfield",
                "Oldbury",
                "Perry Barr",
                "Sheldon",
                "Smethwick",
                "Solihull",
                "Stourbridge",
                "Sutton Coldfield",
                "Tettenhall",
                "Tipton",
                "Walsall",
                "Ward End",
                "Wednesbury",
                "West Bromwich",
                "Willenhall",
                "Wolverhampton",
                "Woodgate Valley"
              ]
            },
            "watch": {
              "type": "string",
              "title": "Watch",
              "enum": [
                "Red",
                "Blue",
                "White",
                "Green",
                "Orange",
                "Purple"
              ],
              "enumNames": [
                "Red",
                "Blue",
                "White",
                "Green",
                "Orange",
                "Purple"
              ]
            },
            "rollNum": {
              "type": "number",
              "title": "Roll number of patient carer"
            }
          }
        },
        "patient": {
          "type": "object",
          "title": "Patient details",
          "description": "The questions in this section relate to the patient which received care.",
          "required": [],
          "properties": {
            "patientName": {
              "type": "string",
              "title": "Patient name"
            },
            "patientGender": {
              "type": "string",
              "title": "Patient gender",
              "default": "MALE",
              "enum": [
                "MALE",
                "FEMALE"
              ],
              "enumNames": [
                "Male",
                "Female"
              ]
            },
            "patientAge": {
              "type": "number",
              "title": "Age of patient"
            },
            "address": {
              "type": "string",
              "title": "Address"
            },
            "collapse": {
              "type": "string",
              "title": "Did the patient collapse?",
              "default": "NO",
              "enum": [
                "NO",
                "NOT_WITNESSED",
                "WITNESSED"
              ],
              "enumNames": [
                "No",
                "Yes (but not witnessed by the carer)",
                "Yes (as witnessed by the carer)"
              ]
            },
            "signsOfLife": {
              "type": "string",
              "title": "Did the patient show any signs of life on arrival?",
              "default": "YES",
              "enum": [
                "YES",
                "NO"
              ],
              "enumNames": [
                "Yes",
                "No"
              ]
            }
          }
        },
        "airway": {
          "type": "object",
          "title": "Any airway treatments?",
          "required": [],
          "properties": {
            "gurgling": {
              "type": "string",
              "default": "NOT_APPLICABLE",
              "title": "Gurgling noise from airway?",
              "enum": [
                "NOT_APPLICABLE",
                "TURNED_ON_SIDE",
                "SUCTION",
                "OTHER",
                "NONE"
              ],
              "enumNames": [
                "No gurgling noise - not applicable",
                "Patient turned on their side",
                "Suction",
                "Other",
                "None"
              ]
            },
            "snoring": {
              "type": "string",
              "default": "NOT_APPLICABLE",
              "title": "Snoring noise from airway?",
              "enum": [
                "NOT_APPLICABLE",
                "RECOVERY_POSITION",
                "CHIN_LIFT",
                "JAW_THRUST",
                "NP_TUBE_7",
                "NP_TUBE_6",
                "NP_TUBE_5",
                "NP_TUBE_4",
                "NP_TUBE_3",
                "OTHER",
                "NONE"
              ],
              "enumNames": [
                "No snoring noise - not applicable",
                "Recovery Position",
                "Chin Lift",
                "Jaw Thrust",
                "NP Tube (size 7)",
                "NP Tube (size 6)",
                "NP Tube (size 5)",
                "NP Tube (size 4)",
                "NP Tube (size 3)",
                "Other",
                "None"
              ]
            },
            "cspine": {
              "type": "string",
              "title": "C-Spine?",
              "default": "CLEAR",
              "enum": [
                "CLEAR",
                "MILS"
              ],
              "enumNames": [
                "Clear",
                "MILS"
              ]
            },
            "choking": {
              "type": "string",
              "title": "Choking?",
              "default": "NOT_APPLICABLE",
              "enum": [
                "NOT_APPLICABLE",
                "COUGH",
                "BACK_SLAPS",
                "ABDOMINAL_THRUSTS",
                "COMPRESSIONS",
                "OTHER"
              ],
              "enumNames": [
                "No choking - not applicable",
                "Encourage cough",
                "Back slaps",
                "Adbominal/Chest thrusts",
                "Chest compressions (CPR)",
                "Other"
              ]
            },
            "airwayStatus": {
              "type": "string",
              "title": "Airway Status",
              "default": "TALKING",
              "enum": [
                "TALKING",
                "CLEAR",
                "PARTIALLY_OBSTRUCTED",
                "COMPLETELY_OBSTRUCTED"
              ],
              "enumNames": [
                "Talking",
                "Clear",
                "Partially obstructed",
                "Completely obstructed"
              ]
            }
          }
        },
        "breathing": {
          "type": "object",
          "title": "Any breathing treatments?",
          "required": [],
          "properties": {
            "alwaysBreathing": {
              "type": "string",
              "title": "Was the patient breathing at all times?",
              "default": "YES",
              "enum": [
                "YES",
                "POCKET_MASK",
                "MOUTH_TO_MOUTH",
                "BAG_VALVE_MASK",
                "OTHER"
              ],
              "enumNames": [
                "Yes",
                "No - Pocket Mask",
                "No - Mouth to mouth",
                "No - Bag-valve-mask (two operator BVM)",
                "No - Other"
              ]
            },
            "chestInjury": {
              "type": "string",
              "title": "Any chest injury found?",
              "default": "NO",
              "enum": [
                "NO",
                "FLAIL_CHEST",
                "OPEN_SUCKING",
                "CHEST_WALL_BRUISING",
                "OTHER"
              ],
              "enumNames": [
                "No",
                "Yes - flail chest",
                "Yes - open sucking chest wound",
                "Yes - chest wall bruising",
                "Yes - other"
              ]
            },
            "chestInjuryTreatment": {
              "type": "string",
              "title": "What treatment was provided for the chest injury?",
              "enum": [
                "CHEST_SEAL",
                "PATIENT_POSITIONING",
                "OXYGEN",
                "OTHER",
                "NONE"
              ],
              "enumNames": [
                "Chest seal",
                "Patient positioning",
                "Oxygen",
                "Other",
                "None"
              ]
            },
            "normalBreathing": {
              "type": "string",
              "title": "Any treatment when breathing normally?",
              "default": "NO",
              "enum": [
                "NO",
                "15_MIN_REBREATHER_MASK",
                "OTHER"
              ],
              "enumNames": [
                "No",
                "Yes - oxygen @ 15 l/min via non-rebreather mask",
                "Yes - other"
              ]
            },
            "respiratoryArrival": {
              "type": "string",
              "title": "Initial respiratory rate on arrival"
            },
            "respiratoryHandover": {
              "type": "string",
              "title": "Respiratory rate on handover"
            }
          }
        },
        "circulation": {
          "type": "object",
          "title": "Circulation",
          "required": [],
          "properties": {
            "bleeding": {
              "type": "string",
              "default": "NO",
              "title": "Any external compressible bleeding?",
              "enum": [
                "NO",
                "DIRECT_PRESSURE",
                "ELEVATION",
                "PRESSURE_DRESSING",
                "INDIRECT_PRESSURE",
                "TOURNIQUET",
                "CELOX",
                "OTHER",
                "NONE"
              ],
              "enumNames": [
                "No",
                "Yes - direct pressure applied",
                "Yes - elevation",
                "Yes - pressure dressing applied",
                "Yes - indirect pressure applied",
                "Yes - tourniquet applied",
                "Yes - haemostatic agent (Celox)",
                "Yes - other treatment provided",
                "Yes - but no treatment provided"
              ]
            },
            "shock": {
              "type": "string",
              "title": "Was the patient in shock?",
              "default": "NO",
              "enum": [
                "NO",
                "LEG_ELEVATION",
                "TOPS",
                "SPLINTED",
                "OTHER"
              ],
              "enumNames": [
                "No",
                "Elevation of legs (pelvic injury not suspected)",
                "Pelvic binder (TOPS)",
                "Fractures splinted",
                "Other"
              ]
            },
            "radialPulse": {
              "type": "string",
              "title": "Radial Pulse",
              "default": "NOT_ASSESSED",
              "enum": [
                "NOT_ASSESSED",
                "PRESENT",
                "NOT_PRESENT"
              ],
              "enumNames": [
                "Not assessed",
                "Present",
                "Not present"
              ]
            },
            "pulseArrival": {
              "type": "number",
              "title": "Pulse rate on arrival (BPM)"
            },
            "pulseHandover": {
              "type": "number",
              "title": "Pulse rate on handover (BPM)"
            },
            "bloodPressure": {
              "type": "string",
              "title": "Blood pressure (if recorded)"
            },
            "temperature": {
              "type": "string",
              "title": "Temperature (if recorded)"
            },
            "capillaryRefill": {
              "type": "number",
              "title": "Capillary refill time (seconds)"
            }
          }
        },
        "disability": {
          "type": "object",
          "title": "Disability",
          "required": [],
          "properties": {
            "limbMovement": {
              "type": "array",
              "title": "Which limbs were seen to move?",
              "items": [
                {
                  "type": "string",
                  "default": "LEFT_ARM",
                  "title": "Left arm",
                  "key": "LEFT_ARM"
                },
                {
                  "type": "string",
                  "default": "RIGHT_ARM",
                  "title": "Right arm",
                  "key": "RIGHT_ARM"
                },
                {
                  "type": "string",
                  "default": "LEFT_LEG",
                  "title": "Left leg",
                  "key": "LEFT_LEG"
                },
                {
                  "type": "string",
                  "default": "RIGHT_LEG",
                  "title": "Right leg",
                  "key": "RIGHT_LEG"
                }
              ]
            },
            "avpuArrival": {
              "type": "string",
              "title": "AVPU score on arrival",
              "default": "ALERT",
              "enum": [
                "ALERT",
                "VERBAL",
                "PAIN",
                "UNRESPONSIVE"
              ],
              "enumNames": [
                "Alert",
                "Verbal",
                "Pain",
                "Unresponsive to pain"
              ]
            },
            "avpuHandover": {
              "type": "string",
              "title": "AVPU score on handover",
              "default": "ALERT",
              "enum": [
                "ALERT",
                "VERBAL",
                "PAIN",
                "UNRESPONSIVE"
              ],
              "enumNames": [
                "Alert",
                "Verbal",
                "Pain",
                "Unresponsive to pain"
              ]
            },
            "painArrival": {
              "type": "array",
              "description": "How did the carer assess the paitient's pain when they first met?",
              "title": "Pain-score on arrival",
              "default": 1,
              "items": [
                {
                  "label": "0",
                  "value": 0,
                  "icon": "",
                  "hint": "No pain"
                },
                {
                  "label": "1",
                  "value": 1,
                  "icon": "",
                  "hint": "Slight pain"
                },
                {
                  "label": "2",
                  "value": 2,
                  "icon": "",
                  "hint": "Moderate pain"
                },
                {
                  "label": "3",
                  "value": 3,
                  "icon": "",
                  "hint": "Severe pain"
                }
              ]
            },
            "painHandover": {
              "type": "array",
              "description": "How did the carer assess the patient's pain at handover?",
              "title": "Pain-score at handover",
              "default": 1,
              "items": [
                {
                  "label": "0",
                  "value": 0,
                  "icon": "",
                  "hint": "No pain"
                },
                {
                  "label": "1",
                  "value": 1,
                  "icon": "",
                  "hint": "Slight pain"
                },
                {
                  "label": "2",
                  "value": 2,
                  "icon": "",
                  "hint": "Moderate pain"
                },
                {
                  "label": "3",
                  "value": 3,
                  "icon": "",
                  "hint": "Severe pain"
                }
              ]
            }
          }
        },
        "exposure": {
          "type": "object",
          "title": "Exposure",
          "required": [],
          "properties": {
            "exposure": {
              "type": "string",
              "default": "NOT_NECESSARY",
              "title": "Was the patient treated for exposure?",
              "enum": [
                "NOT_NECESSARY",
                "BACK_AND_SIDES",
                "BLANKET",
                "PROTECTED",
                "FULLY_UNDRESSED",
                "LOGROLLED",
                "C_COLLAR",
                "OTEER",
                "NO"
              ],
              "enumNames": [
                "No treatment necessary",
                "Yes - back and sides of casualty checked",
                "Yes - blanket",
                "Yes - Protected from heat loss",
                "Yes - fully undressed",
                "Yes - logrolled",
                "Yes - C-collar applied for extrication",
                "Yes - other",
                "No"
              ]
            }
          }
        },
        "burns": {
          "type": "object",
          "title": "Burns",
          "required": [],
          "properties": {
            "burns": {
              "type": "boolean",
              "title": "Did the patient suffer burns?"
            },
            "burnTime": {
              "type": "string",
              "title": "Approximate time the burn occurred"
            },
            "burnCause": {
              "type": "string",
              "title": "Cause of burns",
              "enum": [
                "CHEMICAL",
                "FLAME",
                "FRICTION",
                "RADIATION",
                "COLD",
                "ELECTRICAL",
                "SCALD",
                "OTHER"
              ],
              "enumNames": [
                "Chemical",
                "Flame",
                "Friction",
                "Radiation",
                "Cold",
                "Electrical",
                "Scald (steam/boiling water)",
                "Other"
              ]
            },
            "burnDepth": {
              "type": "string",
              "title": "Burn depth (estimate)",
              "enum": [
                "FULL",
                "PARTIAL",
                "SUPERFICIAL",
                "OTHER"
              ],
              "enumNames": [
                "Full thickness",
                "Partial thickness",
                "Superficial",
                "Other"
              ]
            },
            "burnArea": {
              "type": "number",
              "title": "Estimated body surface area burnt (%)",
              "minimum": 0,
              "maximum": 100,
              "step": 5
            },
            "burnTreatment": {
              "type": "string",
              "title": "Burns - treatment",
              "enum": [
                "COOLED",
                "CLINGFILM",
                "FACE_GEL",
                "NONE"
              ],
              "enumNames": [
                "Cooled for at least 10 minutes",
                "Clingfilm",
                "Face gel dressing",
                "No treatment applied"
              ]
            },
            "treatmentTime": {
              "type": "string",
              "title": "Approximate time the treatment was applied"
            }
          }
        },
        "cpr": {
          "type": "object",
          "title": "CPR",
          "required": [],
          "properties": {
            "cprGiven": {
              "type": "boolean",
              "title": "Was CPR given by the patient carer?"
            },
            "cprTime": {
              "type": "string",
              "title": "Time CPR started"
            },
            "aedTime": {
              "type": "string",
              "title": "Time AED applied"
            },
            "numShocks": {
              "type": "number",
              "title": "Number of shocks delivered"
            },
            "signsReturn": {
              "type": "string",
              "title": "Time signs of life returned"
            }
          }
        },
        "medication": {
          "type": "object",
          "title": "Medication",
          "required": [],
          "properties": {
            "medicationGiven": {
              "type": "string",
              "title": "Was medication given be the patient carer?",
              "default": "NO",
              "enum": [
                "NO",
                "OXYGEN",
                "GLUCOSE",
                "GLUCAGON",
                "OTHER"
              ],
              "enumNames": [
                "No",
                "Oxygen",
                "Glucose",
                "Glucagon",
                "Other"
              ]
            }
          }
        },
        "summary": {
          "type": "object",
          "title": "Summary",
          "required": [],
          "properties": {
            "clinicalNotes": {
              "type": "string",
              "title": "Clinical Notes"
            },
            "handover": {
              "type": "string",
              "title": "Who was the patient handed over to? (Emergency service and name of person)"
            }
          }
        }
      }
    },
